{
  "name": "BC Web Studio",
  "version": "1.0.0",
  "scripts": {
    "build": "npx tailwindcss -i ./src/style.css -o ./dist/style.css --minify && npx @11ty/eleventy",
    "dev": "concurrently \"npx tailwindcss -i ./src/style.css -o ./dist/style.css --watch\" \"npx @11ty/eleventy --serve\""
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.1.2",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0"
  }
}
